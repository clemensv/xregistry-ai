{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.json"
    },
    {
      "route": "/mcpproviders",
      "rewrite": "/mcpproviders/index.json"
    },
    {
      "route": "/mcpproviders/",
      "rewrite": "/mcpproviders/index.json"
    },
    {
      "route": "/agentcardproviders",
      "rewrite": "/agentcardproviders/index.json"
    },
    {
      "route": "/agentcardproviders/",
      "rewrite": "/agentcardproviders/index.json"
    },
    {
      "route": "/mcpproviders/*/servers",
      "rewrite": "/mcpproviders/{provider}/servers/index.json"
    },
    {
      "route": "/mcpproviders/*/servers/",
      "rewrite": "/mcpproviders/{provider}/servers/index.json"
    },
    {
      "route": "/agentcardproviders/*/agentcards",
      "rewrite": "/agentcardproviders/{provider}/agentcards/index.json"
    },
    {
      "route": "/agentcardproviders/*/agentcards/",
      "rewrite": "/agentcardproviders/{provider}/agentcards/index.json"
    },
    {
      "route": "/*",
      "headers": {
        "Cache-Control": "public, max-age=300",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    "": "application/json"
  },
  "defaultDocument": "index.json"
} 